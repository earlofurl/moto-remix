{
  "name": "moto-remix",
  "description": "",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "db:prepare-migration": "npm run shadow-db:start && prisma migrate dev --create-only --skip-seed && npm run shadow-db:stop",
    "db:deploy-migration": "prisma migrate deploy",
    "dev": "run-p dev:*",
    "dev:css": "cross-env NODE_ENV=development npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
    "shadow-db:start": "docker-compose up -d && dotenv -- node ./wait-shadow-db-setup.js",
    "shadow-db:stop": "docker-compose stop",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "run-s setup:db",
    "setup:db": "prisma generate && prisma migrate deploy",
    "start": "remix-serve build",
    "start:ci": "dotenv -- remix-serve build",
    "seed": "prisma db seed",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css"
  },
  "prettier": {
    "singleAttributePerLine": true
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@headlessui/react": "^1.7.2",
    "@heroicons/react": "^1.0.6",
    "@prisma/client": "^4.3.1",
    "@remix-run/node": "^1.7.2",
    "@remix-run/react": "^1.7.2",
    "@remix-run/serve": "^1.7.2",
    "@sendgrid/mail": "^7.7.0",
    "@supabase/supabase-js": "^1.35.7",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tanstack/match-sorter-utils": "^8.1.1",
    "@tanstack/react-table": "^8.5.13",
    "convert": "^4.6.3",
    "dayjs": "^1.11.5",
    "froebel": "^0.21.3",
    "headers-polyfill": "^3.1.0",
    "i18next": "^21.9.2",
    "i18next-browser-languagedetector": "^6.1.5",
    "i18next-fs-backend": "^1.1.5",
    "i18next-http-backend": "^1.4.2",
    "mapbox-gl": "^2.10.0",
    "msw": "^0.47.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.6",
    "react-map-gl": "^7.0.19",
    "remix-i18next": "^4.1.1",
    "remix-params-helper": "^0.4.10",
    "remix-utils": "^3.3.0",
    "slugify": "^1.6.5",
    "tiny-invariant": "^1.2.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.2",
    "@remix-run/eslint-config": "^1.7.2",
    "@tailwindcss/forms": "^0.5.3",
    "@types/eslint": "^8.4.6",
    "@types/mapbox-gl": "^2.7.5",
    "@types/node": "^18.7.19",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/uuid": "^8.3.4",
    "binode": "^1.0.5",
    "c8": "^7.12.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.2",
    "dotenv-cli": "^6.0.0",
    "esbuild": "^0.15.9",
    "eslint": "^8.23.1",
    "eslint-config-galex": "^4.2.2",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "prisma": "^4.3.1",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "uuid": "^9.0.0"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "schema": "app/database/schema.prisma",
    "seed": "ts-node --require tsconfig-paths/register app/database/seed.server.ts"
  }
}
